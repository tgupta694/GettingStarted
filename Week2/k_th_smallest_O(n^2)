# include<iostream>
using namespace std;

void swap(int &a, int &b){
  int temp;
  temp=a;
  a=b;
	b=temp;
   }
int main(){
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n; i++){cin>>a[i];} // input in the array
  for(int i=0; i<n-1;i++){
	       for(int j=0; j<n; j++){
			      if(a[j-1]>a[j])
			      swap(a[j-1],a[j]);}}	                    	                    
	 int k;
	 cout<<"k smallest element";
   cin>>k;
   cout<< "  "<<a[k-1]<<endl;
	 cout<<" for kth largest element"<<  " ";
   cin>>k;
   cout<<" "<<a[n-k];
	 	 }                    
	 
